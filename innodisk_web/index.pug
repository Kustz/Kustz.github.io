					var hr = parseInt((offsetTime / 60 / 60) % 24); // 時
					var day = parseInt(offsetTime / 24 / 60 / 60); // 時
					if(endTime3 > nowTime) {
						if(sec<10) {
							sec = "0"+sec;
						}
						//- console.log(day, hr, min, sec);
						$('.tb.d span').text(day);
						$('.tb.h span').text(hr);
						$('.tb.m span').text(min);
						$('.tb.s span').text(sec);
					}
				}, 1000);

			})